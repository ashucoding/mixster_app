<% if !user_signed_in? %>
<div class="jumbotron">
<h1>Your Favorite Drinks Reviewed</h1>
<p>Track your favorite drinks with Mixster!</p>
<p><%= link_to "Sign Up To Write A Review", new_user_registration_path, class: "btn  btn-primary btn-lg" %></p>
</div>

<%end%>

<div class="row">
  <% @users.each do |user| %>
    <div class="col-xs-6 col-md-3">
    <%= user.email %>
    </div>
  <%end%>
  </div>